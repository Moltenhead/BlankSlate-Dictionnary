<p id="notice"><%= notice %></p>

<h1>Concepts</h1>

<table>
  <thead>
    <tr>
      <th>Meanings</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @instances.each do |instance| %>
      <tr>
        <td><%= instance.meanings %></td>
        <td><%= link_to 'Show', instance %></td>
        <td><%= link_to 'Edit', edit_concept_path(instance) %></td>
        <td><%= link_to 'Destroy', instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Concept', new_concept_path %>
